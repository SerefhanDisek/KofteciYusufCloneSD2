<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="KofteciYusufCloneSD2.Navigations.GirisYap">
    
    <ContentPage.Content>
        <StackLayout VerticalOptions="StartAndExpand">
            <Label Text="KullanıcıAdı" />
            <Entry x:Name="kullanıcıAdıEntry" Placeholder="kullanıcıAdı" />
            <Label Text="Şifre" />
            <Entry x:Name="şifreEntry" IsPassword="True" />
            <Button Text="Giriş Yap" Clicked="GirisYap_Click" />
            <Label x:Name="messageLabel" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>